#include <iostream>
using namespace std;

int main() {
	int t;
	std::cin >> t;
	while(t--)
	{
	    int A,B,C;
	    cin>>A>>B>>C;
	    if(A>50)
	        std::cout << "A" << std::endl;
	    else if(B>50)
	        std::cout << "B" << std::endl;
	    else if(C>50)
	        std::cout << "C" << std::endl;
	    else
	        std::cout << "NOTA" << std::endl;
	}
	return 0;
}
